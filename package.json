{
  "name": "wanikani-tatoeba",
  "version": "0.1.0",
  "author": "Myuwi",
  "description": "Adds a \"More Context Sentences from Tatoeba.org\" section to WaniKani's vocabulary pages",
  "license": "ISC",
  "homepage": "https://github.com/myuwi/wanikani-tatoeba#readme",
  "bugs": {
    "url": "https://github.com/myuwi/wanikani-tatoeba/issues"
  },
  "scripts": {
    "prebuild": "pnpm type-check && pnpm test",
    "build": "esbuild src/index.ts --bundle --target=es2017 --banner:js=\"$(pnpm --silent header)\n\" --outfile=dist/index.js",
    "header": "ts-node src/header.ts",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "wanikani",
    "tatoeba",
    "userscript"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "esbuild": "^0.16.10",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "type-fest": "^3.4.0",
    "typescript": "^4.9.4"
  }
}
